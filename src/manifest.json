{
  "manifest_version": 2,
  "name": "Cog - System Info Viewer",
  "short_name": "Cog",
  "description": "Display lots of useful hardware and network info",
  "version": "0.0.7",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "assets/icon_16.png",
    "36": "assets/icon_36.png",
    "48": "assets/icon_48.png",
    "78": "assets/icon_78.png",
    "96": "assets/icon_96.png",
    "128": "assets/icon_128.png",
    "144": "assets/icon_144.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "system.cpu",
    "system.display",
    "system.memory",
    "system.network",
    "system.storage"
  ]
}
